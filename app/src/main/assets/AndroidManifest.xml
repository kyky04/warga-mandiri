<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.metamorph.shop"
    android:versionCode="112"
    android:versionName="31.0"
    platformBuildVersionCode="25" platformBuildVersionName="7.1.1"
    >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.MenuActivity" />
        <activity android:name=".activities.WebViewActivity"></activity>
        <activity android:name=".activities.CartActivity"></activity>
    </application>

</manifest>